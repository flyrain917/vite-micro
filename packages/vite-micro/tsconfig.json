{
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitOverride": true,
    "noUnusedLocals": false, // error:  is declared but its value is never read.
    "noImplicitAny": false,
    

    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [],
    "typeRoots": [],
    "paths": {
      "@/*": [
        "./src*"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "types/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "mock",
    "build",
    "dist",
  ]
}